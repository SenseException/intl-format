{
    "timeout": 60,
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "infection-log.txt"
    },
    "mutators": {
        "@default": true,
        "IdenticalEqual": {
            "ignore": [
                "*"
            ]
        },
        "NotIdenticalNotEqual": {
            "ignore": [
                "*"
            ]
        },
        "CastInt": {
            "ignore": [
                "*"
            ]
        }
    }
}
